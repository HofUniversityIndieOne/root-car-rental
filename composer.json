{
	"repositories": [
		{ "type": "composer", "url": "https://composer.typo3.org/" },
		{ "type": "git", "url": "https://github.com/HofUniversityIndieOne/site-package-basics.git" },
		{ "type": "git", "url": "https://github.com/HofUniversityIndieOne/site-package-car-rental.git" },
		{ "type": "git", "url": "https://github.com/HofUniversityIndieOne/car-rental.git" }
	],
	"name": "typo3/cms-base-distribution",
	"description" : "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0+",
	"require": {
		"typo3/cms": "^8.7",
		"helhum/typo3-console": "^4.4",
		"hof-university-indie/car-rental": "dev-master",
		"hof-university-indie/site-package-basics": "dev-master",
		"in2code/femanager": "^3.3",
		"typo3/cms-felogin": "^8.7",
		"dmitryd/typo3-realurl": "^2.3"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "web"
		}
	},
	"require-dev": {
		"typo3/cms-tstemplate": "^8.7",
		"typo3/cms-t3editor": "^8.7",
		"friendsoftypo3/extension-builder": "dev-master"
	},
	"scripts": {
		"post-install-cmd": [
			"/bin/sh build/composer-install.sh"
		],
		"post-package-install": [
			"/bin/sh build/composer-install.sh"
		],
		"post-package-update": [
			"/bin/sh build/composer-install.sh"
		],
		"post-package-uninstall": [
			"/bin/sh build/composer-install.sh"
		]
	}
}
